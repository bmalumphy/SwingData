# platform :ios, '9.0'

target 'Swing Data' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Swing Data
  pod 'Charts'
  
  target 'Swing DataTests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Charts'
  end

  target 'Swing DataUITests' do
    inherit! :search_paths
    # Pods for testing
    pod 'Charts'
  end

end
